{
  "include": ["./src","src/**/*"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "outDir": "./dist"
  },
  "exclude": ["**/*.spec.ts","node_modules"]
}
